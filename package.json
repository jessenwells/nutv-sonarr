{
  "name": "3",
  "version": "0.1.5",
  "private": true,
  "dependencies": {
    "antd": "^3.20.7",
    "firebase": "^6.3.5",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lazyload": "^2.6.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "build-css": "stylus -c src/ --out src/",
    "watch-css": "npm run build-css && stylus -c -w src/ --out src/",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "stylus": "^0.54.5"
  }
}
